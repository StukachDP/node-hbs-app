                            <label>Magazine publisher</label><br>
                            <input name="publisher" class="field" id="create-field"/><br><br>
                            <label>Magazine publisher in English</label><br>
                            <input name="publisherEng" class="field" id="create-field"/><br><br>
                            <label>Scientific Directions</label><br>
                            <input name="scientificDirections" class="field" id="create-field"/><br><br>
